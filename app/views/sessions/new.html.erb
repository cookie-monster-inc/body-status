<% if @alerts %>
  <% @alerts.each do |error| %>
    <%= error %><br>
  <% end %>
<% end %>

<%= form_tag('/sessions') do |f| %>
  <%= label_tag :email %>
  <%= text_field_tag 'email', :email %>
  <%= label_tag :password %>
  <%= password_field_tag 'password', :password %>
  <%= submit_tag 'Log in' %>
<% end %>